*.pkl filter=lfs diff=lfs merge=lfs -text
# Ensure consistent line endings
* text=auto

# Specific file types
*.js text eol=lf
*.jsx text eol=lf
*.css text eol=lf
*.html text eol=lf
*.json text eol=lf
*.md text eol=lf
*.sh text eol=lf

# .gitattributes
* text=auto
*.sh text eol=lf
*.py text eol=lf
*.js text eol=lf
*.css text eol=lf
*.html text eol=lf
*.bat text eol=crlf
